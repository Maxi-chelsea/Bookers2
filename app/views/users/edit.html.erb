<h2>User info</h2>
  <%= form_with model:@user, local:true do |f| %>
      <div class="form-group">
        <h4>Name</h4>
        <%= f.text_field :Name %>
      </div>
      <div class="form-group">
        <h4>Introduction</h4>
        <%= f.text_area :body %>
      </div>
        <%= f.submit 'Update User' %>
      <% end %>
</div>
